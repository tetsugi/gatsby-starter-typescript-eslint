{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "target": "esnext",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["./*"],
      "types/*": ["types/*"]
    }
  },
  "include": [
    "./src/**/*", "./configs/**/*", "./types/**/*"
  ]
}
